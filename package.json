{
  "name": "swimlane",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "start": "npm ci && npm run build && serve -s dist -l 10000"
  },
  "dependencies": {
    "vue": "^3.4.21",
    "vite": "^5.2.8",
    "@vitejs/plugin-vue": "^5.0.4",
    "serve": "^14.2.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "prettier": "^3.2.5"
  }
}
